  },
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "typeChallenges.workspaceFolder": "/home/deep/projects/personal/dsa/type-challenges/problems",
  "vs-kubernetes": {
    "vs-kubernetes.minikube-show-information-expiration": "2023-05-16T16:39:52.635Z"
  },
  "vscode-neovim.neovimInitVimPaths.linux": "~/.vimrc",
  "dart.experimentalRefactors": true,
  "editor.tabSize": 2,
  "workbench.colorTheme": "Catppuccin Mocha",
  "playwright.reuseBrowser": true
}
